{
    "name": "netteam/datatable-bundle",
    "type": "symfony-bundle",
    "description": "NetTeam DataTable Bundle",
    "authors": [
        {
            "name": "NetTeam Solutions",
            "homepage": "http://netteam.pl"
        }
    ],
    "repositories": [
        { "type": "composer", "url": "http://composer.nts/" }
    ],
    "require": {
        "php": ">=5.3.2",
        "symfony/framework-bundle": ">=2.1,<2.3-dev",
        "symfony/property-access": ">=2.1,<2.3-dev",
        "doctrine/orm": "~2.2"
    },
    "require-dev": {
        "mockery/mockery": ">=0.7.2",
        "symfony/form": ">=2.1,<2.3-dev",
        "symfony/browser-kit": ">=2.1,<2.3-dev",
        "liuggio/excelbundle": "1.0.*"
    },
    "autoload": {
        "psr-0": { "NetTeam\\Bundle\\DataTableBundle": "" }
    },
    "minimum-stability": "dev",
    "target-dir": "NetTeam/Bundle/DataTableBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "1.1.x-dev"
        }
    }
}
