"oTableTools": {
    "sSwfPath": "{{ asset('bundles/netteamdatatable/swf/copy_cvs_xls.swf') }}",
    "aButtons": [
    {% for key, export in datatable.exports %}
        {
            "sExtends": "download",
            "sButtonText": "{{ export.getLabel() }}",
            "sUrl": "{{ path(datatable.route, datatable.routeExportParameters(key))|raw }}"
        }
        {{ loop.last ? '' : ','}}
    {% endfor %}
    ]
}
