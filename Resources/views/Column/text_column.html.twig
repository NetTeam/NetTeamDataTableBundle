{% extends "NetTeamDataTableBundle:Column:column.html.twig" %}
{% block columnValue %}
{% set value = record.value|default('')|trans({}, translation_domain|default('messages')) %}
{% if record.raw|default(false) %}
    {{ value|replace({ '\n' : '<br />',  '\r' : '' })|raw }}
{% else %}
    {{ value|replace({ '\n' : '<br />',  '\r' : '' }) }}
{% endif %}
{% endblock %}
