{% for item in value  %}
{% if item is array %}
    {% include "NetTeamDataTableBundle:Column:array_column_recursive.html.twig" with { 'value' : item, 'separator' : separator } %}
{% else %}
    {{ item|replace({ '\n' : '<br />' })|trans }}{% if not loop.last %}{{ separator }}{% endif %}
{% endif %}
{% endfor %}
