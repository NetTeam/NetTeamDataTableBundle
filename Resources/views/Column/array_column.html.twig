{% for item in value.items  %}
{% autoescape false %}
{% if item is array %}
    {% include "NetTeamDataTableBundle:Column:array_column_recursive.html.twig" with { 'value' : item, 'translate' : translate, 'separator': value.separator } %}
{% else %}
    {{ item|e|replace({ '\n' : '<br />' }) }}{% if not loop.last %}{{ value.separator }}{% endif %}
{% endif %}
{% endautoescape %}
{% endfor %}