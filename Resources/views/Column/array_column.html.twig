{% extends "NetTeamDataTableBundle:Column:column.html.twig" %}
{% block columnValue %}
{% for item in record.value  %}
{% if item is array %}
    {% include "NetTeamDataTableBundle:Column:array_column_recursive.html.twig" with { 'value' : item, 'separator': record.separator } %}
{% else %}
    {{ item|replace({ '\n' : '<br />' })|trans }}{% if not loop.last %}{{ record.separator }}{% endif %}
{% endif %}
{% endfor %}
{% endblock %}
